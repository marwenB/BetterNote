<ul class="notebooks">
  <li>
    <a href="<%= notes_url %>">All Notes</a>
  </li>
  <% current_user.notebooks.each do |notebook| %>
    <% next unless notebook.persisted? %>
    <li>
      <a href="<%= notebook_url(notebook) %>">
        <%= notebook.name %>
      </a>
    </li>
  <% end %>
  <li>
    <%= link_to "Add New Notebook", new_notebook_url %>
  </li>
</ul>
