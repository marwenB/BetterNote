<table class="">
  <tr>
    <td class="note-info-field">Title:</td>
    <td class="note-info-data"><%= note.get("title") %></td>
  </tr>
  <tr>
    <td class="note-info-field">Author:</td>
    <td class="note-info-data"><%= note.author.get("username") %></td>
  </tr>
  <tr>
    <td class="note-info-field">Created:</td>
    <td class="note-info-data">
      <%= moment(note.get("created_at")).format('MMM D, YYYY') %>
    </td>
  </tr>
  <tr>
    <td class="note-info-field">Updated:</td>
    <td class="note-info-data">
      <%= moment(note.get("updated_at")).format('MMM D, YYYY') %>
    </td>
  </tr>
  <tr>
    <td class="note-info-field">Notebook:</td>
    <td class="note-info-data"><%= note.notebook.get("name") %></td>
  </tr>
  <tr>
    <td class="note-info-field">Tags:</td>
    <td class="note-info-data">
      <% if (note.noteTags.length > 0) { %>
        <%= note.noteTags.map(function(noteTag) {
          return noteTag.get("tag_name")
        }).join(", ") %>
      <% } else { %>
        No tags
      <% } %>
    </td>
  </tr>
  <tr>
    <td class="note-info-field">Likes:</td>
    <td class="note-info-data">
      <%= note.likes.length %>
    </td>
  </tr>
</table>